{
  "name": "elderissuesdresser-pack",
  "version": "0.2.0",
  "description": "Interactive to help understand issues faced by older adults",
  "main": "src/index.js",
  "scripts": {
    "serve": "webpack serve --hot --mode development",
    "test": "npm run test-unit && npm run test-client",
    "test-unit": "mocha test/model-test.js",
    "test-client": "sh -c \"[ \\\"$CI\\\" = \\\"true\\\" ] && mocha-headless-chrome -f dist/view-test.html --no-sandbox || mocha-headless-chrome -f dist/view-test.html\"",
    "build": "webpack --mode production && webpack --config test/test.webpack.config.js --mode development"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ccnmtl/elderissuesdresser-pack.git"
  },
  "keywords": [
    "webpack",
    "mocha",
    "chai",
    "jquery",
    "client",
    "ctl"
  ],
  "author": {
    "name": "Zarina Mustapha",
    "email": "zarina@columbia.edu"
  },
  "bugs": {
    "url": "https://github.com/ccnmtl/elderissuesdresser-pack/issues"
  },
  "license": "GPL-3.0",
  "greenkeeper": {
    "ignore": [
      "bootstrap"
    ]
  },
  "devDependencies": {
    "chai": "^6.0.0",
    "css-loader": "^7.0.0",
    "eslint": "~9.39.0",
    "eslint-plugin-no-unsafe-innerhtml": "^1.0.16",
    "file-loader": "^6.0.0",
    "globals": "^17.0.0",
    "loglevel": "^1.4.1",
    "mocha": "^11.0.0",
    "mocha-headless-chrome": "^5.1.0",
    "style-loader": "^4.0.0",
    "webpack-dev-server": "^5.0.0"
  },
  "dependencies": {
    "backbone": "1.6.0",
    "bootstrap": "^5.3.4",
    "bootstrap-arrow-buttons": "^1.0.4",
    "jquery": "^4.0.0",
    "underscore": "1.13.1",
    "underscore-template-loader": "^1.0.0",
    "webpack": "^5.10.3",
    "webpack-cli": "~6.0.0"
  }
}
